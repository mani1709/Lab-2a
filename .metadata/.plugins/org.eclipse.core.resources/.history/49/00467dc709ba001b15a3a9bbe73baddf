public class Utils {
	//if we dont want the vectors to change, we can use this class to create a new vector from the return
	
	public Vec3 add(Vec3 a, Vec3 b) {
		double x = a.x + b.x;
		double y = a.y + b.y;
		double z = a.z + b.z;
		return new Vec3(x, y, z);
	}
	
	public void multiply(Vec3 a, Vec3 b) {
		this.x *= s;
		this.y *= s;
		this.z *= s;
	}
}